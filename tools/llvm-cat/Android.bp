//===---------------------------------------------------------------===
// llvm-cat command line tool
//===---------------------------------------------------------------===

cc_binary_host {
    name: "llvm-cat",
    defaults: ["llvm-defaults"],
    srcs: ["llvm-cat.cpp"],
		group_static_libs: true,
    static_libs: [
        "libLLVMAsmParser",
        "libLLVMBitReader",
        "libLLVMBitWriter",
        "libLLVMCore",
        "libLLVMIRReader",
        "libLLVMSupport",
    ],
}
