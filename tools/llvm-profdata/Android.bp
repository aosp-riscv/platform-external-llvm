//===---------------------------------------------------------------===
// llvm-profdata command line tool
//===---------------------------------------------------------------===

cc_binary_host {
    name: "llvm-profdata",
    defaults: ["llvm-defaults", "llvm-generated-headers"],
    srcs: ["llvm-profdata.cpp"],
    static_libs: [
        "libLLVMObject",
        "libLLVMProfileData",
        "libLLVMCore",
        "libLLVMSupport",
    ],
}
