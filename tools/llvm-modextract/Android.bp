cc_binary_host {
    name: "llvm-modextract",
    defaults: ["llvm-defaults"],
    srcs: ["llvm-modextract.cpp"],
    static_libs: [
        "libLLVMBitReader",
        "libLLVMBitWriter",
        "libLLVMCore",
        "libLLVMSupport",
    ],
}
