cc_library_static {
    name: "libLLVMCore",
    defaults: [
        "llvm-defaults",
        "llvm-generated-headers",
    ],
    generated_headers: ["llvm-gen-core"],
    srcs: ["*.cpp"],
}

llvm_tblgen {
    name: "llvm-gen-core",
    in: "AttributesCompatFunc.td",
    outs: ["AttributesCompatFunc.inc"],
}
